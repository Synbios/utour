<%= form_for :session, url: sessions_path, :html => {:role => "form", :class => "form-horizontal"} do |f| %>

<div class="form-group">
  <%= f.text_field :identification_id, :placeholder => "邮箱/手机号", :class => "form-control" %>
</div>

<div class="form-group">
  <%= f.password_field :password, :class => "form-control", :placeholder => "请输入密码" %>
</div>

<div class="form-group">
  <%= f.submit "登录", :class => "btn btn-success full_width" %>
</div>

<% end %>