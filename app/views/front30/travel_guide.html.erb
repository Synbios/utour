<% content_for :javascripts do %>
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
<% end %>

<% content_for :nav_bar do %>
<%= render "layouts/lion_nav" %>
<% end %>

<% content_for :left_links do %>
<% render "left_links" %>
<% end %>

<% content_for :right_sidebar do %>
<%= render "layouts/lion_sidebar_right" %>
<% end %>

<div class="inner-page">
  <div class="container">

    <div class="page-mainbar landing">

      <div class="landing-content">

        <div class="landing-logo">

          <%= image_tag "logoabout.png", style: "width: 160px; margin: 10px auto" %>
        </div>
      </div>
    </div>

    <div class="page-mainbar faq">
      <h3>旅游攻略</h3>
      <div class="faq-content">

        <div class="panel-group">
          <div class="panel panel-default">
            <div class="panel-heading br-yellow">
              <h4 class="panel-title">                
                <a href="http://dp.sina.cn/dpool/tools/flight/index.php?pos=81&vt=4">
                  <h5 style="color: #fff"><i class="fa fa-angle-right"></i>&nbsp; 航班动态</h5>
                </a>
              </h4>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading br-pink">
              <h4 class="panel-title">                
                <a href="http://dp.sina.cn/dpool/tools/forex/index.php?vt=4&pos=95&cid=768">
                  <h5 style="color: #fff"><i class="fa fa-angle-right"></i>&nbsp; 货币兑换</h5>
                </a>
              </h4>
            </div>
          </div>

          <div class="panel panel-default">

            <div class="panel-heading br-blue">
              <h4 class="panel-title">
                <a class="accordion-toggle" data-toggle="collapse" href="#maps">
                  <h5 style="color: #fff"><i class="fa fa-angle-right"></i>&nbsp; 世界地图</h5>
                </a>
              </h4>
            </div>
            <div id="maps" class="panel-collapse collapse" style="background-color: #222; padding: 7px">
              <div class="row">
                <div class="col-xs-4">
                  <div class="box box-md br-orange animated" style="height: 100px">
                    <a href="/maps/europe.pdf">
                      <div class="box-content navigation">
                        <h4 style="color: #fff">欧洲地图</h4>
                      </div>
                    </a>
                  </div>
                </div>

                <div class="col-xs-4">
                  <div class="box box-md br-brown animated" style="height: 100px">
                    <a href="/maps/america.pdf">
                      <div class="box-content navigation">
                        <h4 style="color: #fff">美洲地图</h4>
                      </div>
                    </a>
                  </div>
                </div>

                <div class="col-xs-4">
                  <div class="box box-md br-lblue animated" style="height: 100px">
                    <a href="/maps/austrilia.pdf">
                      <div class="box-content navigation">
                        <h4 style="color: #fff">澳洲地图</h4>
                      </div>
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading br-pink">
              <h4 class="panel-title">                
                <a href="http://fanyi.baidu.com/#auto/en/">
                  <h5><i class="fa fa-angle-right"></i>&nbsp; 语言翻译</h5>
                </a>
              </h4>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading br-red">
              <h4 class="panel-title">                
                <a href="http://m.mafengwo.cn/mdd/">
                  <h5><i class="fa fa-angle-right"></i>&nbsp; 旅游攻略</h5>
                </a>
              </h4>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading br-green">
              <h4 class="panel-title">                
                <a href="http://weather1.sina.cn/?vt=4">
                  <h5><i class="fa fa-angle-right"></i>&nbsp; 世界天气</h5>
                </a>
              </h4>
            </div>
          </div>      
        </div>
      </div>
    </div>
  </div>
</div>
